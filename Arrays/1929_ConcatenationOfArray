class Solution {
    public int[] getConcatenation(int[] nums) {
        int n = nums.length;
        int[] ans = new int[2*n];
        for(int i = 0; i<nums.length;i++){
            ans[i] = nums[i];
        }
        int i = 0;
        while(i<nums.length){
                 for(int j = nums.length;j<ans.length;j++){
                  ans[j] = nums[i];
                  i++;
        }
        }
       
        return ans;
    }
}